<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" type="text/css" href="/css/main.css" />
  <link rel="stylesheet" type="text/css" href="/css/bootstrap.css" />
  <title>Crypto Calculator</title>
  <script src="https://kit.fontawesome.com/5ac842a7e3.js" crossorigin="anonymous"></script>
</head>
<body class="body">

<div class="container">
  <div class="row">
    <div class="effectHolder  col-sm-12 col-lg-4 mx-auto">
      <div class="mainBlock">

        <h1>CryptoCalc</h1>

        <div id="assets"></div>

        <div class="tableEffect">
          <table class="searchBlock">
            <tbody>
              <tr>
                <td class="smartBtn">
                  <div class="iconBtn" id="smartBtn">
                    <i class="fa-solid fa-microchip"></i>
                  </div>
                </td>
                <td class="countBtn">
                  <div class="iconBtn" id="countBtn">
                    <i class="fa-solid fa-calculator"></i>
                  </div>
                </td>
                <td class="field">
                  <input type="text" id="cryptoSymbol" placeholder="symbol" />
                </td>
                <td class="searchBtn">
                  <div id="searchBtn" class="iconBtn">
                    <i class="fa-solid fa-plus"></i>
                  </div>
                </td>
                <td class="addBtn">
                  <div class="iconBtn" id="uploadBtn">
                    <i class="fa-solid fa-arrow-up-from-bracket fa-fade"></i>
                  </div>
                  <input type="file" id="fileInput" accept=".json" style="display: none;" />
                </td>
                <td class="download">
                  <div class="iconBtn" id="downloadBtn">
                    <i class="fa-solid fa-file-arrow-down"></i>
                  </div>
                </td>
              </tr>
            </tbody>
          </table>
        </div>

        <h1>GhostDolphin</h1>

      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="/scripts/core.js"></script>

</body>
</html>